@model IEnumerable<MyResumeOfWPF.PDO.EducationRow>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SchoolnameColumn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MajoyColumn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartColumn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PeriodColumn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StageColumn)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SchoolnameColumn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MajoyColumn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartColumn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PeriodColumn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StageColumn)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EducationRowId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EducationRowId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EducationRowId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
